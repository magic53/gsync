{
  "name": "gsync",
  "description": "Asynchronous control flow with generators",
  "version": "0.8.2",
  "author": "Michael Madgett",
  "main": "lib/gsync.js",
  "scripts": {
    "test": "./node_modules/mocha/bin/mocha --harmony-generators --timeout 10000 test/**",
    "test-coverage": "multi='dot=- html-cov=coverage.html' ./node_modules/mocha/bin/mocha -r blanket --reporter mocha-multi --no-colors --timeout 10000 test/**"
  },
  "keywords": [
    "generators",
    "control flow",
    "process"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/magic53/gsync.git"
  },
  "bugs": {
    "url": "https://github.com/magic53/gsync/issues"
  },
  "config": {
    "blanket": {
      "pattern": "///[\\w-]+\\.js$/",
      "data-cover-never": [
        "node_modules",
        "test"
      ],
      "data-cover-reporter-options": {
        "shortnames": true
      }
    }
  },
  "devDependencies": {
    "blanket": "^1.1.7",
    "debug": "^2.2.0",
    "mocha": "^2.2.5",
    "mocha-multi": "^0.7.1",
    "should": "^6.0.3"
  },
  "license": "MIT"
}
